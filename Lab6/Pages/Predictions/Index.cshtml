@page
@model Lab6.Pages.Predictions.IndexModel

@{
    ViewData["Title"] = "Index";
}

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Prediction[0].FileName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prediction[0].Url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prediction[0].Questions)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Prediction)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FileName)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Questions)
                </td>
                <td>
                    <img src="@item.Url" style="">
                </td>
                <td>
                    <a asp-page="./Delete" asp-route-id="@item.PredictionId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
